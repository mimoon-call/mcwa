{
  "name": "mcwa",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "lint": "eslint . --ext .ts,.tsx",
    "dev": "NODE_ENV=development tsx --import ./loader-register.mjs src/server/index.ts",
    "build:client": "vite build",
    "build:server": "vite build --config vite.config.server.ts",
    "build": "npm run build:client && npm run build:server",
    "start": "NODE_ENV=production node dist/server/index.js",
    "run:seed": "NODE_ENV=development tsx --import ./loader-register.mjs src/seed.ts",
    "run:seed:production": "NODE_ENV=production tsx --import ./loader-register.mjs src/seed.ts",
    "clean": "rm -rf dest node_modules package-lock.json && npm install",
    "clean:branch": "git checkout --orphan temp-branch && git add -A && git commit -m 'Initial commit' && git branch -D main && git branch -m main && git push -f origin main"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.1",
